{
    "type": "object",
    "required": [
        "ovsdb-ip",
        "bridge-name",
        "port-name",
        "remote-ip",
        "key"
    ],
    "properties": {
        "ovsdb-ip": {
            "type": "string",
            "format": "IpAddress",
            "example": "35.232.247.116"
        },
        "bridge-name": {
            "type": "string",
            "format": "str",
            "example": "br-1"
        },
        "port-name": {
            "type": "string",
            "format": "srt",
            "example": "port1"
        },
        "remote-ip": {
            "type": "string",
            "format": "srt",
            "example": "192.168.0.100"
        },
        "key": {
            "type": "string",
            "format": "srt",
            "example": "flow"
        }
    }
}